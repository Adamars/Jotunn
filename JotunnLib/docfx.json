{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "*.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**"
                    ]
                }
            ],
            "shouldSkipMarkup": true,
            "dest": "Documentation/api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.md",
                    "**/*.yml"
                ],
                "src": "Documentation"
            }
        ],
        "resource": [
            {
                "files": [
                    "logo.svg",
                    "favicon.ico"
                ],
                "src": "DocFX",
                "dest": "."
            },
            {
                "files": [
                    "Documentation/images/**/*"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "template": [
            "default",
            "DocFx/darkfx"
        ]
    }
}